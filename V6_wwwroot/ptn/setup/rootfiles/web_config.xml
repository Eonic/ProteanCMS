<?xml version="1.0"?>
<configuration>
  <eonic>
    <web configSource="eonic.web.config"/>
    <cart configSource="eonic.cart.config"/>
    <payment configSource="eonic.payment.config"/>
    <mailinglist configSource="eonic.mailinglist.config"/>
    <theme configSource="eonic.theme.config"/>
    <messagingProviders>
      <providers>
        <add name="CampaignMonitorLocal" className="CampaignMonitor" type="Eonic.Providers.Messaging.CampaignMonitor, Version=5.0.56.0, Culture=neutral, PublicKeyToken=0e5e11efc3341916" />
      </providers>
    </messagingProviders>
  </eonic>
  <system.web>
    <compilation defaultLanguage="vb" debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.DirectoryServices, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
        <add assembly="Microsoft.Web.Administration, Version=7.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
      </assemblies>
    </compilation>
    <customErrors mode="Off"/>
    <authentication mode="Windows"/>
    <authorization>
      <allow users="*"/>
    </authorization>
    <trace enabled="false" requestLimit="10" pageOutput="false" traceMode="SortByTime" localOnly="true"/>
    <sessionState mode="InProc" stateConnectionString="tcpip=127.0.0.1:42424" sqlConnectionString="data source=127.0.0.1;Trusted_Connection=yes" cookieless="false" timeout="20"/>
    <globalization requestEncoding="utf-8" responseEncoding="utf-8"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
    <httpRuntime requestValidationMode="2.0"/>
  </system.web>
  <system.webServer>
    <rewrite>
      <rewriteMaps configSource="rewritemaps.config"/>
      <rules configSource="rewriteRules.config"/>
    </rewrite>
  </system.webServer>
</configuration>