      <endpoint address="https://www.proteancms.com/ewAdminProxy.asmx" binding="basicHttpBinding" bindingConfiguration="ewAdminProxySoap" contract="proteancms.com.ewAdminProxySoap" name="ewAdminProxySoap" />
    </client>
