		<add key="RootPageId" value="1"/>
		<add key="BaseUrl" value=""/>
		<add key="SiteName" value="Website Name" />
		<add key="SiteXsl" value="/ewThemes/Mono/standard.xsl"/>
		<add key="Debug" value="on"/>
		<add key="CompiledTransform" value="off"/>
		<!-- email settings -->
		<add key="MailServer" value="mail01.eonic.co.uk"/>
		<add key="SiteAdminEmail" value="support@eonic.co.uk"/>		
		<!-- library directories-->
		<add key="ImageRootPath" value="/images/"/>
