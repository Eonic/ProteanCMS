<?xml version="1.0"?>
	<web>
		<add key="VersionNumber" value="0.0.0.0"/>
    <!-- database settings -->
		<add key="DatabaseType" value="SQL"/>
		<add key="DatabaseName" value=""/>
		<add key="DatabaseUsername" value=""/>
		<add key="DatabasePassword" value=""/>
