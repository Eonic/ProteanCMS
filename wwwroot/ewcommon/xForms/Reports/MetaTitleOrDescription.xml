<?xml version="1.0" encoding="utf-8"?>
<Content type="xform" name="EditContent">
	<model>
		<instance>
			<Query name="MetaTitleOrDescription" storedProcedure="spGetMetaTitleOrDescription" logActivity="true" output="csv" filePrefix="" fileUID="log">							
				<param name="cType" type="string"  value=""/>
			</Query>
		</instance>
		<submission id="MetaTitleOrDescriptionSubmission" action="/ewcommon/tools/export.ashx?ewcmd=Reports&amp;ewCmd2=MetaTitleOrDescription&amp;reportXsl=MetaTitleOrDescription" method="post" event="return form_check(this)"/>
		<bind nodeset="Query">
			<bind id="cType" nodeset="param[@name='cType']/@value" required="false()" type="string"/>			
			<bind id="lastDownloadId" nodeset="param[@name='lastDownloadID']/@value" required="false()" type="string"/>
			<bind id="format" nodeset="@output" required="true()" type="string"/>
		</bind>
	</model>
	<group ref="MetaTitleOrDescription" class="2col">
		<label>Meta Title/Description</label>
			<group>	
				<select1 bind="cType" appearance="minimal">
					<label>Type</label>
					<item>
						<label>Page</label>
						<value>Page</value>
					</item>
					<item>
						<label>Product</label>
						<value>Product</value>
					</item>
					<value/>
				</select1>
				<select1 bind="format" appearance="minimal" class="required">
					<label>Output Format</label>
					<item>
						<label>CSV</label>
						<value>csv</value>
					</item>
					<item>
						<label>Excel</label>
						<value>xls</value>
					</item>
					<item>
						<label>Raw XML</label>
						<value>rawxml</value>
					</item>
					<value/>
				</select1>
			</group>			
	</group>
	<group ref="SubmitDownload" class="inline">
		<submit submission="" ref="ewSubmit" class="principle">
			<label>Download Report</label>
		</submit>
	</group>
</Content>
